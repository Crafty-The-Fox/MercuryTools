<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MercuryTools.Views.ArrayItem">
    <StackPanel>
        
        <DockPanel Margin="10,5,15,5">
            <TextBlock DockPanel.Dock="Left" Name="TextBlockName" Text="0" Margin="0,0,5,0"/>
            <Rectangle Fill="{DynamicResource VeryLightBrush}" Height="1"/>
        </DockPanel>
                    
        <TextBox Classes="Inset" AcceptsReturn="False" AcceptsTab="False" Margin="10,0,10,10" Padding="10" MinHeight="30" Name="TextBoxValue" UndoLimit="0"/>
        
    </StackPanel>
</UserControl>
